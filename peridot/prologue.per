class Fixint
	def +(obj a,obj b)
		{return Int(a.in+b.in);}
	end
	def -(obj a,obj b)
		{return Int(a.in-b.in);}
	end
	def *(obj a,obj b)
		{return Int(a.in*b.in);}
	end
	def /(obj a,obj b)
		{return Int(a.in/b.in);}
	end
	def puts(obj a)
		{printf("%i\n",a.in);}
	end
end
class Array
	def [](obj a,obj b)
  	{return a.ary[b.in];}
	end
	def []=(obj a,obj b,obj c)
  	{return a.ary[b.in]=c;}
	end
end

